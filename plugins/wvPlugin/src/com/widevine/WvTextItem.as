package com.widevine{	public class WvTextItem{		private var _startTime:Number;		private var _duration:Number;		private var _subTitle:String;				public function WvTextItem(startTime:Number, duration:Number, subTitle:String):void{			_startTime = startTime;			_duration = duration;			_subTitle = subTitle;						trace(_startTime + ", " + _duration + "," + _subTitle);		}				public function checkForSubTitle(timeIndex:Number):String{			var endTime:Number = _startTime + _duration;			if (_startTime <= timeIndex && timeIndex <= endTime){				return _subTitle;			}else{				return null;			}		}				public function getStartTime():Number{			return _startTime;		}				public function getDuration():Number{			return _duration;		}			}}